{
    "env": {
      "node": true
    },
    "extends": ["eslint:recommended", "prettier"],
    "rules": {
      "no-console": "off",
      "indent": ["error", 4, {
        "SwitchCase": 1,
        "flatTernaryExpressions": true
      }],
      "quotes": ["error", "single"],
      "semi": ["error", "always"]
    }
  }